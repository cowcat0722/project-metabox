            $('#choiceMovieNone').show();
            $('#choiceMovieList').hide();
        } else {
            $('#choiceMovieNone').hide();
            $('#choiceMovieList').show();
        }

        //극장선택개수 카운팅
        var brchChoiCnt = 0;
        $("#choiceBrchList .bg").each(function (idx) {
            if ($.trim($(this).html()) == "") {
                brchChoiCnt++;
            }
        });
